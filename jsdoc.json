{
  "source": {
      "include": ["src/", "package.json", "README.md"],
      "includePattern": ".js$",
      "excludePattern": "(node_modules/|docs)"
  },

  "plugins": [
    "plugins/markdown",
    "typedef-import"
  ],

  "markdown": {
    "idInHeadings": true
  },
  "templates": {
      "cleverLinks": false,
      "monospaceLinks": true,
      "useLongnameInNav": false,
      "showInheritedInNav": true
  },

  "opts": {
      "encoding": "utf8",
      "readme": "./README.md",
      "destination": "docs/",
      "recurse": true,
      "verbose": true,
      "template": "node_modules/docdash",
      "theme_opts": {
      }
  }
}
