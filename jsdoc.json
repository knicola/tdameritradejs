{
  "source": {
    "include": ["src/", "package.json", "README.md"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "tags": {
    "allowUnknownTags": ["typicalname"]
  },
  "plugins": [
    ".doc_template/jsdoc-mods"
  ],
  "markdown": {
    "idInHeadings": true
  },
  "opts": {
    "template": "node_modules/docdash",
    "encoding": "utf8",
    "readme": "./README.md",
    "destination": "docs/",
    "recurse": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": true,
    "useLongnameInNav": false,
    "showInheritedInNav": true
  },
  "docdash": {
    "collapse": true,
    "search": true,
    "nameInOutputPath": false,
    "versionInOutputPath": false,
    "typedefs": true,
    "sort": false
  }
}
